{
    "name": "zhylon/megapi-client",
    "description": "This package can be used to interact with the MegAPI.",
    "keywords": [
        "TobyMaxham",
        "zhylon"
    ],
    "homepage": "https://github.com/Zhylon/megapi-client",
    "license": "MIT",
    "authors": [
        {
            "name": ":TobyMaxham",
            "email": "git@maxham.de",
            "homepage": "https://maxham.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.2",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.21"
    },
    "autoload": {
        "psr-4": {
            "Zhylon\\MegapiClient\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zhylon\\MegapiClient\\MegApiServiceProvider"
            ]
        }
    },
    "scripts": {
        "cs-fix": "./vendor/bin/pint src/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
